<div class="process-nav">
  <button class="process-nav__trigger show-for-small-only"
          data-toggle="process-nav-content">
    <span class="fa fa-caret-down process-nav__trigger__icon"></span>
    <% if current_page.data.activepage == "info" %>
      <div class="process-nav__link process-nav__link--info">
        <svg width="20" height="20">
          <use xlink:href="#shape-icon-info" />
        </svg>
        <span>Informació</span>
      </div>
    <% end %>
    <% if current_page.data.activepage == "proposals" %>
      <div class="process-nav__link process-nav__link--proposals">
        <svg width="20" height="20">
          <use xlink:href="#shape-icon-proposals" />
        </svg>
        <span>Propostes</span>
      </div>
    <% end %>
    <% if current_page.data.activepage == "actions" %>
      <div class="process-nav__link process-nav__link--actions">
        <svg width="20" height="20">
          <use xlink:href="#shape-icon-actions" />
        </svg>
        <span>Resultats</span>
      </div>
    <% end %>
    <% if current_page.data.activepage == "debates" %>
      <div class="process-nav__link process-nav__link--debates">
        <svg width="20" height="20">
          <use xlink:href="#shape-icon-debates" />
        </svg>
        <span>Debats</span>
      </div>
    <% end %>
    <% if current_page.data.activepage == "meetings" %>
      <div class="process-nav__link process-nav__link--meetings">
        <svg width="20" height="20">
          <use xlink:href="#shape-icon-meetings" />
        </svg>
        <span>Cites presencials</span>
      </div>
    <% end %>
  </button>
  <div class="row process-nav__content" id="process-nav-content"
       data-toggler=".is-active">
    <% if current_page.data.activephase == "phase1" %>
    <ul class="columns large-12">
      <li class="<% if current_page.data.activepage == "info" %>is-active
                 <% end %>">
        <a href="/phase1_info" class="process-nav__link process-nav__link--info
          <% if current_page.data.activepage == "info" %>is-active<% end %>">
          <svg width="20" height="20">
            <use xlink:href="#shape-icon-info" />
          </svg>
          <span>Informació</span>
        </a>
      </li>
      <li class="<% if current_page.data.activepage == "meetings" %>is-active
                 <% end %>">
        <a href="/phase1_meetings" class="process-nav__link process-nav__link--meetings
          <% if current_page.data.activepage == "meetings" %>is-active<% end %>">
          <svg width="20" height="20">
            <use xlink:href="#shape-icon-meetings" />
          </svg>
          <span>Cites presencials</span>
        </a>
      </li>
    </ul>
    <% elsif current_page.data.activephase == "phase2" %>
    <ul class="columns large-12">
      <li class="<% if current_page.data.activepage == "proposals" %>is-active
                 <% end %>">
        <a href="/phase2_proposals" class="process-nav__link process-nav__link--proposals
          <% if current_page.data.activepage == "proposals" %>is-active<% end %>">
          <svg width="20" height="20">
            <use xlink:href="#shape-icon-proposals" />
          </svg>
          <span>Propostes</span>
        </a>
      </li>
      <li class="<% if current_page.data.activepage == "debates" %>is-active
                 <% end %>">
        <a href="/phase2_debates" class="process-nav__link process-nav__link--debates
          <% if current_page.data.activepage == "debates" %>is-active<% end %>">
          <svg width="20" height="20">
            <use xlink:href="#shape-icon-debates" />
          </svg>
          <span>Debats</span>
        </a>
      </li>
      <li class="<% if current_page.data.activepage == "meetings" %>is-active
                 <% end %>">
        <a href="/phase2_meetings" class="process-nav__link process-nav__link--meetings
          <% if current_page.data.activepage == "meetings" %>is-active<% end %>">
          <svg width="20" height="20">
            <use xlink:href="#shape-icon-meetings" />
          </svg>
          <span>Cites presencials</span>
        </a>
      </li>
      <li class="about-link <% if current_page.data.activepage == "about" %>is-active
                 <% end %>">
        <a href="/phase2_about" class="process-nav__link process-nav__link--info
          <% if current_page.data.activepage == "about" %>is-active<% end %>">
          <span>Sobre esta fase</span>
        </a>
      </li>
    </ul>
    <% elsif current_page.data.activephase == "phase3" %>
    <ul class="columns large-12">
      <li class="<% if current_page.data.activepage == "proposals" %>is-active
                 <% end %>">
        <a href="/phase2_proposals" class="process-nav__link process-nav__link--proposals
          <% if current_page.data.activepage == "proposals" %>is-active<% end %>">
          <svg width="20" height="20">
            <use xlink:href="#shape-icon-proposals" />
          </svg>
          <span>Propostes</span>
        </a>
      </li>
      <li class="<% if current_page.data.activepage == "debates" %>is-active
                 <% end %>">
        <a href="/phase2_debates" class="process-nav__link process-nav__link--debates
          <% if current_page.data.activepage == "debates" %>is-active<% end %>">
          <svg width="20" height="20">
            <use xlink:href="#shape-icon-debates" />
          </svg>
          <span>Debats</span>
        </a>
      </li>
      <li class="<% if current_page.data.activepage == "meetings" %>is-active
                 <% end %>">
        <a href="/phase2_meetings" class="process-nav__link process-nav__link--meetings
          <% if current_page.data.activepage == "meetings" %>is-active<% end %>">
          <svg width="20" height="20">
            <use xlink:href="#shape-icon-meetings" />
          </svg>
          <span>Cites presencials</span>
        </a>
      </li>
      <li class="about-link <% if current_page.data.activepage == "about" %>is-active
                 <% end %>">
        <a href="/phase2_about" class="process-nav__link process-nav__link--info
          <% if current_page.data.activepage == "about" %>is-active<% end %>">
          <span>Sobre esta fase</span>
        </a>
      </li>
    </ul>
    <% else %>
    <ul class="columns large-12">
      <li class="<% if current_page.data.activepage == "info" %>is-active
                 <% end %>">
        <a href="/info" class="process-nav__link process-nav__link--info
          <% if current_page.data.activepage == "info" %>is-active<% end %>">
          <svg width="20" height="20">
            <use xlink:href="#shape-icon-info" />
          </svg>
          <span>Informació</span>
        </a>
      </li>
      <li class="<% if current_page.data.activepage == "proposals" %>is-active
                 <% end %>">
        <a href="/proposals" class="process-nav__link process-nav__link--proposals
          <% if current_page.data.activepage == "proposals" %>is-active<% end %>">
          <svg width="20" height="20">
            <use xlink:href="#shape-icon-proposals" />
          </svg>
          <span>Propostes</span>
        </a>
      </li>
      <li class="<% if current_page.data.activepage == "actions" %>is-active
                 <% end %>">
        <a href="/actions" class="process-nav__link process-nav__link--actions
          <% if current_page.data.activepage == "actions" %>is-active<% end %>">
          <svg width="20" height="20">
            <use xlink:href="#shape-icon-actions" />
          </svg>
          <span>Resultats</span>
        </a>
      </li>
      <li class="<% if current_page.data.activepage == "debates" %>is-active
                 <% end %>">
        <a href="/debates" class="process-nav__link process-nav__link--debates
          <% if current_page.data.activepage == "debates" %>is-active<% end %>">
          <svg width="20" height="20">
            <use xlink:href="#shape-icon-debates" />
          </svg>
          <span>Debats</span>
        </a>
      </li>
      <li class="<% if current_page.data.activepage == "meetings" %>is-active
                 <% end %>">
        <a href="/meetings" class="process-nav__link process-nav__link--meetings
          <% if current_page.data.activepage == "meetings" %>is-active<% end %>">
          <svg width="20" height="20">
            <use xlink:href="#shape-icon-meetings" />
          </svg>
          <span>Cites presencials</span>
        </a>
      </li>
    </ul>
    <% end %>
  </div>
</div>